{
  "name": "audeets-api-projects",
  "version": "1.0.0",
  "private": true,
  "author": "Benoit Quette",
  "main": "app.js",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint  --ext .js --ext .jsx .",
    "test": "npm run coverage && npm run check-coverage",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive \"./test/unit/**/*Spec.js\"",
    "check-coverage": "istanbul check-coverage --statement 85",
    "start": "node --env-file=.env ./bin/www",
    "start:dev": "nodemon --env-file=.env ./bin/www"
  },
  "dependencies": {
    "amqplib": "^0.4.2",
    "async": "^2.0.1",
    "base-64": "^0.1.0",
    "body-parser": "~1.15.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.2",
    "os": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "chai": "^3.5.0",
    "chai-things": "^0.2.0",
    "connect": "^3.4.1",
    "cross-env": "^2.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "finalhandler": "^0.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "nodemon": "^3.0.3",
    "proxyquire": "^1.7.10",
    "serve-static": "^1.11.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-class-properties",
      "@babel/plugin-transform-object-rest-spread"
    ]
  }
}
